{
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
    	"psr-4": {
    		"FourKites\\": "."
    	}
    },
    "autoload-dev": {
        "psr-4": {
